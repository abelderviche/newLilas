<?xml version='1.0' encoding='utf-8'?>
<widget id="com.groween.laslilas" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Las Lilas</name>
    <description>
        Venta de semen premium.
    </description>
    <author email="info@groween.com" href="https://www.groween.com">
        Groween.com
    </author>
    <content src="index.html" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="false" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <plugin name="cordova-plugin-battery-status" source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-camera" source="npm" spec="~2.1.1" />
    <plugin name="cordova-plugin-media-capture" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" source="npm" spec="~1.0.2" />
    <plugin name="cordova-plugin-contacts" source="npm" spec="~2.0.1" />
    <plugin name="cordova-plugin-device" source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-device-motion" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-device-orientation" source="npm" spec="~1.0.2" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-file" source="npm" spec="~4.1.1" />
    <plugin name="cordova-plugin-file-transfer" source="npm" spec="~1.5.0" />
    <plugin name="cordova-plugin-geolocation" source="npm" spec="~2.1.0" />
    <plugin name="cordova-plugin-globalization" source="npm" spec="~1.0.3" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~1.3.0" />
    <plugin name="cordova-plugin-media" source="npm" spec="~2.2.0" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-splashscreen" source="npm" spec="~3.2.1" />
    <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.2" />
    <plugin name="cordova-plugin-vibration" source="npm" spec="~2.1.0" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    <icon src="icon.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="icon.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="icon.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="icon.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="icon.png" />
    <icon gap:platform="blackberry" src="www/res/icon/blackberry/icon-80.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="www/res/icon/blackberry/icon-80.png" />

    <icon gap:platform="ios" height="20" src="www/res/icon/ios/Icon-App-20x20@1x.png" width="20" />
    <icon gap:platform="ios" height="29" src="www/res/icon/ios/Icon-App-29x29@1x.png" width="29" />
    <icon gap:platform="ios" height="40" src="www/res/icon/ios/Icon-App-40x40@1x.png" width="40" />
    <icon gap:platform="ios" height="57" src="www/res/icon/ios/Icon-App-57x57@1x.png" width="57" />
    <icon gap:platform="ios" height="60" src="www/res/icon/ios/Icon-App-60x60@1x.png" width="60" />
    <icon gap:platform="ios" height="72" src="www/res/icon/ios/Icon-App-72x72@1x.png" width="72" />
    <icon gap:platform="ios" height="76" src="www/res/icon/ios/Icon-App-76x76@1x.png" width="76" />

    
    <icon gap:platform="ios" height="40" src="www/res/icon/ios/Icon-App-20x20@2x.png" width="40" />
    <icon gap:platform="ios" height="58" src="www/res/icon/ios/Icon-App-29x29@2x.png" width="58" />
    <icon gap:platform="ios" height="80" src="www/res/icon/ios/Icon-App-40x40@2x.png" width="80" />
    <icon gap:platform="ios" height="114" src="www/res/icon/ios/Icon-App-57x57@2x.png" width="114" />
    <icon gap:platform="ios" height="120" src="www/res/icon/ios/Icon-App-60x60@2x.png" width="120" />
    <icon gap:platform="ios" height="144" src="www/res/icon/ios/Icon-App-72x72@2x.png" width="144" />
    <icon gap:platform="ios" height="152" src="www/res/icon/ios/Icon-App-76x76@2x.png" width="152" />

    <icon gap:platform="ios" height="60" src="www/res/icon/ios/Icon-App-20x20@3x.png" width="60" />
    <icon gap:platform="ios" height="87" src="www/res/icon/ios/Icon-App-29x29@3x.png" width="87" />
    <icon gap:platform="ios" height="120" src="www/res/icon/ios/Icon-App-40x40@3x.png" width="120" />
    <icon gap:platform="ios" height="201" src="www/res/icon/ios/Icon-App-57x57@3x.png" width="201" />
    <icon gap:platform="ios" height="180" src="www/res/icon/ios/Icon-App-60x60@3x.png" width="180" />
    <icon gap:platform="ios" height="216" src="www/res/icon/ios/Icon-App-72x72@3x.png" width="216" />
    <icon gap:platform="ios" height="228" src="www/res/icon/ios/Icon-App-76x76@3x.png" width="228" />


    <icon gap:platform="webos" src="www/res/icon/webos/icon-64.png" />
    <icon gap:platform="winphone" src="www/res/icon/windows-phone/icon-48.png" />
    <icon gap:platform="winphone" gap:role="background" src="www/res/icon/windows-phone/icon-173-tile.png" />
    <gap:splash src="splash.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="splash.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="splash.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="splash.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="splash.png" />
    <gap:splash gap:platform="blackberry" src="www/res/screen/blackberry/screen-225.png" />
    <gap:splash gap:platform="ios" height="480" src="www/res/screen/ios/screen-iphone-portrait.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="www/res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="www/res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="www/res/screen/ios/screen-ipad-portrait.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="www/res/screen/ios/screen-ipad-landscape.png" width="1024" />
    <gap:splash gap:platform="winphone" src="www/res/screen/windows-phone/screen-portrait.jpg" />
    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <access origin="mailto:*" launch-external="yes" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
            <splash src="splash.png" density="land-hdpi"/>
            <splash src="splash.png" density="land-ldpi"/>
            <splash src="splash.png" density="land-mdpi"/>
            <splash src="splash.png" density="land-xhdpi"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="SplashScreenDelay" value="3000" />
</widget>
